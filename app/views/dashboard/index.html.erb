<h2>Users</h2>
<% for user in @users %>
  <p><%= user.email %> <%= user.admin ? "(admin)" : "" %>
     <%= link_to "Edit User", edit_user_path(user) %>
      <% unless user.email == current_user.email %>
        <%= link_to "Delete User", user, method: :delete, data: { confirm: 'Are you sure?' }  %>
      <% end %>
<% end %>
</p>
<%= link_to 'New User', new_user_path %><p/>
